div
	h1.bottom_border.pbm.mbm Final Result
	- if @result1 and @result2
		h3.blue
			i
				|Based on the test you have taken here are your results. Please call 
				span.maroon 408-555-1212 
				|for explanations.
		table.table.table-striped
			thead
				tr
					th.tc colspan="3" Part 1
					/ th
					th.tc colspan="3"  Part 2

			tbody
				tr
					td.tr Differentiation
					td.tc= @dif1
					td= @dif1_rating
					/ td.tc.blue DIF
					td Differentiation
					td.tc= @dif2 
					td.tr= @dif2_rating

				tr
					td.tr Discerning Individuality
					td.tc= @total_dim_i1 
					td= @dim_i1_rating
					/ td.tc.blue DIM I
					td Self Regard
					td.tc= @total_dim_i2 
					td.tr= @dim_i2_rating

				tr
					td.tr Practicality
					td.tc= @total_dim_e1 
					td= @dim_e1_rating
					/ td.tc.blue DIM E
					td Role Awareness
					td.tc= @total_dim_e2 
					td.tr= @dim_e2_rating

				tr
					td.tr Discerning Systems or Concepts
					td.tc= @total_dim_s1 
					td= @dim_s1_rating
					/ td.tc.blue DIM S
					td Self Concept
					td.tc= @total_dim_s2 
					td.tr= @dim_s2_rating

				tr
					td.tr Sense of Proportion
					td.tc= @dim1 
					td= @dim1_rating
					/ td.tc.blue DIM
					td Sense of Proportion
					td.tc= @dim2 
					td.tr= @dim2_rating

				tr
					td.tr Sense of Meaning
					td.tc= @dimper1
					td= @dimper1_rating
					/ td.tc.blue DIM%
					td Sense of Meaning
					td.tc= @dimper2
					td.tr= @dimper2_rating

				tr
					td.tr Empathy
					td.tc= @total_int_i1
					td= @total_int_i1_rating
					/ td.tc.blue INT I
					td Discerning self relevance 
					td.tc= @total_int_i2
					td.tr= @total_int_i2_rating

				tr
					td.tr Discerning practical relevance
					td.tc= @total_int_e1
					td= @total_int_e1_rating
					/ td.tc.blue INT E
					td Discerning practical relevance
					td.tc= @total_int_e2
					td.tr= @total_int_e2_rating

				tr
					td.tr Discerning conceptual relevance
					td.tc= @total_int_s1
					td= @total_int_s1_rating
					/ td.tc.blue INT S
					td Discerning self-concept relevance
					td.tc= @total_int_s2
					td.tr= @total_int_s2_rating

				tr
					td.tr Integration
					td.tc= @int1
					td= @int1_rating
					/ td.tc.blue INT
					td Integration
					td.tc= @int2
					td.tr= @int2_rating

				tr
					td.tr Relative Integration
					td.tc= @intper1
					td= @intper1_rating
					/ td.tc.blue INT%
					td Relative Integration
					td.tc= @intper2
					td.tr= @intper2_rating

				tr
					td.tr Dimensional Integration
					td.tc= @di1
					td= @di1_rating
					/ td.tc.blue DI
					td Dimensional Integration
					td.tc= @di2
					td.tr= @di2_rating

				tr
					td.tr Distortion
					td.tc= @total_dis1
					td= @total_dis1_rating
					/ td.tc.blue DIS
					td Distortion
					td.tc= @total_dis2
					td.tr= @total_dis2_rating

				tr
					td.tr Attitude Index
					td.tc= @alper1.to_s + ' %'
					td= @alper1_rating
					/ td.tc.blue AI%
					td Attitude Index
					td.tc= @alper2.to_s + ' %'
					td.tr= @alper2_rating

				tr
					td.tr Part 1 Validity
					td.tc= '%0.2f' % @rho1
					td= @total_rho123_rating
					/ td.tc.blue RHO
					td Part 2 Validity
					td.tc= '%0.2f' % @rho2
					td.tr= @total_rho456_rating

		table.table.table-striped
			thead
				tr
					th.tc colspan="3" Part 1 Balance Indicators
					th
					th.tc colspan="3"  Part 2 Balance Indicators

			tbody
				tr
					td People
					td.tc= @total_bali1
					td.tr= @total_bali1_rating
					/ td BAL I
					td Self - Esteem
					td.tc= @total_bali2
					td.tr= @total_bali2_rating

				tr
					td Tasks
					td.tc= @total_bale1
					td.tr= @total_bale1_rating
					/ td BAL E
					td Self - Concept
					td.tc= @total_bale2
					td.tr= @total_bale2_rating

				tr
					td Decision Making
					td.tc= @total_bals1
					td.tr= @total_bals1_rating
					/ td BAL S
					td Self - Image
					td.tc= @total_bals2
					td.tr= @total_bals2_rating

	- else
		h4.alert.alert-error Take test 1 and test 2 for results